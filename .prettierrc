{
  "printWidth": 120,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "parser": "typescript",
  "overrides": [
    {
      "files": ["*.yaml"],
      "options": {
        "bracketSpacing": false,
        "parser": "yaml"
      }
    },
    {
      "files": ["*.json"],
      "options": {
        "parser": "json"
      }
    }
  ]
}
